$dark_bgPrimary = #2F2F2F
$dark_bgSecondary = #2b2b2b
$dark_borderColor = #212121
$dark_borderColorLight = white
$dark_textColor = #FFFFFF
$dark_transition = all .2 ease

.layout-main__negative{
  background-color: $dark_bgPrimary;
  color: $dark_textColor;
}
.layout-main__negative{
    .navbar, .navbar .links, .navbar .logo path, .search-box input, .dropdown-wrapper .nav-dropdown, .front-teaser, .ribbon{
  background-color: $dark_bgSecondary;
  color: $dark_textColor;
  fill: $dark_textColor;
}
}


